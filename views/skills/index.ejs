<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Dev Skills</title>
</head>
<body>
    <a href="/">Home</a>
    <h1>Skills</h1>
    <ul>
      <% skills.forEach(function(s, idx) { %>
        <li>
          <a href="/skills/<%= idx %>"> <%= s.skill %></a>
            - 
          <%= s.exp ? 'Yes!' : 'Not Yet!' %>
        </li>
      <% }); %>
    </ul>  
</body>
</html>